{
  "name": "wplace-autobot",
  "version": "0.0.1",
  "description": "WPlace Auto-Image",
  "main": "dist/auto-image.standalone.js",
  "type": "module",
  "scripts": {
    "build": "node build/esbuild.config.js",
    "build:bookmarklet": "node build/esbuild.config.js --target=bookmarklet",
    "build:userscript": "node build/esbuild.config.js --target=userscript",
    "build:standalone": "node build/esbuild.config.js --target=standalone",
    "build:all": "node build/esbuild.config.js --target=all",
    "dev:userscript": "node build/esbuild.config.js --target=userscript --watch",
    "dev:standalone": "node build/esbuild.config.js --target=standalone --watch",
    "serve:dist": "http-server dist -p 5173 -c-1 --cors",
    "tunnel:start": "cloudflared tunnel run --token $CLOUDFLARED_TOKEN",
    "tunnel:quick": "cloudflared tunnel --url http://localhost:5173",
    "dev:tunnel": "run-p dev:standalone serve:dist tunnel:start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --ignore-unknown --write .",
    "format:check": "prettier --ignore-path .gitignore --ignore-path .prettierignore --ignore-unknown --check .",
    "clean": "rm -rf dist/*",
    "docs:api": "documentation build src/Auto-Image.js -f md -o docs/api/README.md"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "documentation": "^14.0.3",
    "esbuild": "^0.19.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "http-server": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0"
  },
  "keywords": [
    "userscript",
    "bookmarklet",
    "wplace",
    "autobot"
  ],
  "author": "WPlace AutoBot Team",
  "license": "MIT"
}
